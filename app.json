{
  "expo": {
    "name": "TimeTable",
    "slug": "time-table",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image/icon.png",
    "splash": {
      "image": "./assets/image/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "irb1s.ru",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "irb1s.ru",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/image/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/image/favicon.png"
    }
  }
}
